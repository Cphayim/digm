{
  "name": "@cphayim/digm-react",
  "version": "0.1.8",
  "author": "Cphayim <i@cphayim.me>",
  "description": "Encapsulation and enhancement of Digital Model renderer for React, provide hooks and components",
  "license": "ISC",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "main": "dist/index.cjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "vite build --mode=development --watch",
    "build": "tsc --noEmit && vite build"
  },
  "dependencies": {
    "@cphayim/digm-core": "workspace:*",
    "@cphayim/digm-shared": "workspace:*"
  },
  "devDependencies": {
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.6",
    "classnames": "^2.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "peerDependencies": {
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Cphayim/digm.git",
    "directory": "packages/react"
  },
  "bugs": {
    "url": "https://github.com/Cphayim/digm/issues"
  }
}
